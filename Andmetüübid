function checkTemp(temp) {
    console.log("Temp on "+temp)

if ( temp >= 21 && temp <= 40 ) {
    console.log("0");

} if ( temp > 0 && temp <= 20 ) {
    console.log("-1");

} else if ( temp > 40 && temp < 60) 
    console.log("1");


}
